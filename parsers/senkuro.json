{
   "id":65216,
   "version":3,
   
   
   "title":"Senkuro",
   "host":"https://senkuro.com",
   "public_link":"https://senkuro.com/browse/manga",
   "folder":"senkuro",
   "name":"senkuro",
   "language":"ru",
   "encoding":"utf-8",
   "chapters_order":false,
   
   "icon_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/img/senkuro.png",
	"self_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/parsers/senkuro.json",
	"self_link2": "https://fant.my1.com/mwparser/mwx/json/senkuro.json",
	"catalog_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/catalogs/senkuro.zip",
    "file_type": "sqlite",
	
   
   "author":{
      "name":"MWX",
      "email":"fant-net@ya.com"
   },
   "manga_list_complete":{
      "iterator":{
         "base_url":"http://45.95.202.233/api/senkuro/mangas?page=",
         "append_nums":{
            "from":1,
            "to":330,
            "step":1
         }
      },
	  "link_corrector": [
            {
                "match": "/chapters",
                "text": ""
            }
        ],
      "add_manga":{
         "next":"\"formats\"",
		 
         "title":{
            "skip":"\"titles\":[",
            "token1":"\"content\":\"",
            "token2":"\""
         },
         "link":{
            "token1":"\"slug\":\"",
            "token2":"\"",
			"replace": { "prefix": "https://senkuro.com/manga/"}
         },
        "uniq":{
            "token1":"\"slug\":\"",
            "token2":"\""
		}
      },
		"rating_auto_inc": {
			"start": "20000",
			"inc": -1
		}
   },
   
   
   "manga_search_complete": {		
        "search_link":"http://45.95.202.233/api/senkuro/manga/search?q=$query$",
		"query_type": "GET",
		
		 "add_manga":{
		 
         "next":"\"node\"",
		 
          "title":{
            "before":"\"lang\":\"RU\"",
            "token1":"\"content\":\"",
            "token2":"\""
			
			
			
         },
		 
          "link":{
            "token1":"\"slug\":\"",
            "token2":"\"",
			"replace": { "prefix": "https://senkuro.com/manga/"}
         },
		 
		 "summary": {  "token1": "\"translitionStatus\":\"", "token2":"\"",
                "replace":[
				{"match": "IN_PROGRESS","text": "Статус перевода: Переводится"},
				{"match": "FINISHED","text": "Статус перевода: Завершён"},
				{"match": "FROZEN","text": "Статус перевода: Заморожен"},
				{"match": "ABANDONED","text": "Статус перевода: Заброшен"},
				{"match":"<.*?>","text":""}

				] },
				
         "cover":{
			"skip":"\"cover\"",
		    "token1":"\"url\":\"https://shiro.senkuro.net/",
            "token2":"\"",
			"replace": { "prefix": "https://shiro.senkuro.net/"}
         }
      }
		},	
		
		
   
   "manga_complete": { 
	   "headers": {
			"User-Agent": "MangaWatcherX",
			"Referer": "https://senkuro.com/"
		},
      "title":{
         "after":"property=\"og:title\"", 
         "token1":"content=\"",
         "token2":"\"" ,
		 
		 "replace": [
				{"match": "Манга","text": ""},
				{"match": "Манхва","text": ""},
				{"match": "Читать","text": ""},
				{"match": "/","text": "|"},
				{"match":"<.*?>","text":""}

				]},
      
      "uniq":{
         "token1":"property=\"og:url\" content=\"https://senkuro.com/manga/",
         "token2":"\""
      },
	  
	  
      "cover":{
		 "skip":"\"contentUrl\":",
         "token1":"\"https://",
         "token2":"\"",
		"replace": {
				"prefix": "https://"
				}
		},
	  
	  
	  "status": { 
		  	"complete": { 
			 "start": ">Статус перевода</span>",
             "end": "</li>",
			   "values": [ "Завершена", "Переведена", "Сингл"] },
			
            "ongoing": { 
			 "start": ">Статус перевода</span>",
             "end": "</li>",
               "values": [ "Выпускается", "Переведена", "Переводится"] 
				
			 }
		},
			 
			 
      "summary":{
         "after":"name=\"description\"",
         "token1":"content=\"",
         "token2":"\">",
         "replace":{
            "match":"<.*?>",
            "text":""
         }
      },
      "add_tags":   { 	
	   "start": "\\\"genres\\\":",
             "end": "\\\"series\\\":",
	        "token1": "RU\\\",\\\"content\\\":\\\"", "token2": "\\\"" },
	  
	  
      "is_mature":{
         "values":[
            "content\\\":\\\"эротика\\\"",
            "content\\\":\\\"юри\\\"",
            "content\\\":\\\"яой\\\""
			
         ]
      },
	  
	
	  
	 

        "chapters_from_page": {
            "after": "property=\"og:url\"",
            "token1": "content=\"https://senkuro.com/manga/",
            "token2": "\"",
            "replace": { "prefix": "http://45.95.202.233/api/senkuro/manga/chapters/" }
            
        },
	  
	  
		
		"add_chapter":{
        "start": "[",
		"end": "]",
        
		 
         "next": "\"num",
		 
		                 
         "link":{
			"skip": "\"num",
            "token1":"\"slug\":\"",
            "token2":"\"",
			"replace":
		        [
		    { "prefix": "https://senkuro.com/manga/%%url%%/chapters/" },
			{ "match": "http://45.95.202.233/api/senkuro/manga/chapters/", "text": ""}
		        ]
         },
		 
		"uniq":{
            "skip": "\"num",
            "token1":"ber\":",
            "token2":"}",
			"replace": [
			{ "match": "\"(.*?)\",\"slug\":\"(.*?)\",\"volume\":\"(.*?)\".*?",	
			"text": "$3/$1"},
			{ "match": "null", "text": ""}
			]
        },
		 
        "title":{
            "skip": "\"num",
            "token1":"ber\":",
            "token2":"}",
			"replace": [
			{ "match": "\"(.*?)\",\"slug\":\"(.*?)\",\"volume\":\"(.*?)\".*?",	
			"text": "Том: $3 Глава: $1"},
			{ "match": "null", "text": ""}
			]
        }
      }
	  
	  
   },
   
   "chapter_complete":{
    "add_numeration": true,
        "headers": {
			"Accept": "*/*",
			"Referer": "https://senkuro.com/"
		},
     
      "images":true,
    "add_pages":

	{
		"start": "MangaChapter\\\",\\\"pages",
		"after": "\\\"url\\\":\\\"",
		"token1": "https://",
		"token2": "\\\"",
		"end": "branchId",
		"replace":
		[{
				"prefix": "https://"
			}, {
				"match": "Сдесь могла быть ваша реклама",
				"text": ""
			},
			{
				"match": "Сдесь могла быть ваша реклама",
				"text": ""
			}
		]
	}

	},
	
	
