{
   "id":40228,
   "version":23,
   
   "old_sites": ["mangapoisk.ru","mangapoisk.com","mangapoisk.org" ],
   
   
   "title":"MangaPoisk",
   "host":"https://mangapoisk.net",
   "public_link":"https://mangapoisk.net/manga-list",
   "folder":"mangapoisk",
   "name":"mangapoisk",
   "language":"ru",
   "encoding":"utf-8",
   "chapters_order":false,
   
   "icon_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/img/mangapoisk.png",
	"self_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/parsers/mangapoisk.json",
	"self_link2": "https://fant.my1.com/mwparser/mwx/json/mangapoisk.json",
	"catalog_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/catalogs/mangapoisk.zip",
    "file_type": "sqlite",
	
   
   "author":{
      "name":"Veopot",
      "email":"fant-net@ya.com"
   },
   "manga_list_complete":{
      "iterator":{
         "base_url":"https://mangapoisk.net/manga?sortBy=popular&page=",
         "append_nums":{
            "from":0,
            "to":281,
            "step":1
         }
      },
      "add_manga":{
         "next":"class=\"post-description\">",
		 
         "title":{
            "skip":"href=\"",
            "token1":"\">",
            "token2":"</"
         },
         "link":{
            "token1":"href=\"",
            "token2":"\""
         },
         "cover":{
		    "before":"href=\"",
            "token1":"data-src=\"",
            "token2":"\""
         }
      },
		"rating_auto_inc": {
			"start": "20000",
			"inc": -1
		}
   },
   
   
   "manga_search_complete": {		
        "search_link":"https://mangapoisk.net/search?q=$query$&page=$page$",
		"query_type": "GET",
		
		 "add_manga":{
		 
         "next":"<article class=\"flex-item",
		 
         "title":{
            "skip":"href=\"",
            "token1":"class=\"card-title js-card-title\">",
            "token2":"</"
         },
         "link":{
            "token1":"href=\"",
            "token2":"\""
         },
		 
		 "summary": { "skip":"href=\"", "token1": "<p class=\"card-text\">", "token2": "</p>",
                "replace":{"match":"<.*?>", "text":""} },
				
         "cover":{
			"skip":"href=\"",
		    "token1":"https://static2.mangapoisk.net/posters/",
            "token2":"\"",
			"replace": { "prefix": "https://static2.mangapoisk.net/posters/"}
         }
      }
		},	
		
		
   
   "manga_complete":[
   
    { "condition": "/chaptersList?",
	   "headers": {
			"User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/88.0.3987.163 Safari/537.36",
			"Referer": "https://mangapoisk.net"
		},
      "title":{
         
         "token1":"class=\"post-name\">",
         "token2":"</h1>" ,
         "replace":{"match":"<.*?>","text":""} },
      
      "uniq":{
         
         "token1":"let manga = '",
         "token2":"';"
      },
	  
	  
      "cover":{
        "skip":"class=\"row\">",
         "token1":"src=\"",
         "token2":"\""
      },
      
	  
	  
	  "rating":{
        "skip":"class=\"post-info\">",
         "token1":"class=\"ratesCount\">",
         "token2":"</"
      },
	  
	  
	  "status": { 
		  	"complete": { 
			 "start": ">Статус:<",
             "end": ">Жанр:<",
			   "values": [ "Завершена", "Переведена", "Сингл"] },
			
            "ongoing": { 
			"start": ">Статус:<",
             "end": ">Жанр:<",
                "value": "Выпускается" }
			 },
			 
			 
      "summary":{
         "skip":">Описание:<",
         "token1":"class=\"manga-description entry\">",
         "token2":"</div>",
         "replace":{
            "match":"<.*?>",
            "text":""
         }
      },
      "add_genres":{
         "start":">Жанр:<",
         "end":">Год:<",
         "token1":"\">",
         "token2":"</"
      },
      "is_mature":{
         "values":[
            "эротика",
            "сэйнэн",
            "юри",
			"этти",
            "яой"
         ]
      },
	  
	"chapters_from_pages": { 
		"start": "class=\"chapters-infinite-pagination",
		 "end": "</ul>",
		"after": "class=\"page-link",
		"token1": "\">",
		"token2": "</", 
		"replace" :  { "prefix" : "%%url%%/chaptersList?page=" } 
		
		},
	  
	  
	  
		
		"add_chapter":{
        "start": "<head>",
		 "end": "class=\"chapters-infinite-pagination\"",
        
		 
         "next": "chapter-item",
		 
		                 
         "link":{
			
            "token1":"href=\"",
            "token2":"\"",
			"replace": {"prefix" : "https://mangapoisk.net"}
         },
		 
		"uniq":{
            "skip":"href=\"",
            "token1":"/chapter/",
            "token2":"\""
        },
		 
        "title":{
            "skip":"href=\"",
            "token1":"class=\"chapter-title\">",
            "token2":"</a>",
			"replace": { "match": "</span>",	"text": ""}
        }
      }
	  
	  
   },
   { "condition": "",
	   "headers": {
			"User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/88.0.3987.163 Safari/537.36",
			"Referer": "https://mangapoisk.net"
		},
      "title":{
         
         "token1":"class=\"post-name\">",
         "token2":"</h1>" ,
         "replace":{"match":"<.*?>","text":""} },
      
      "uniq":{
         "skip":"class=\"row\">",
         "token1":"class=\"post-name\">",
         "token2":"</"
      },
	  
	  
      "cover":{
        "skip":"class=\"row\">",
         "token1":"src=\"",
         "token2":"\""
      },
	  
	  "rating":{
        "skip":"class=\"post-info\">",
         "token1":"class=\"ratesCount\">",
         "token2":"</"
      },
      
	  "status": { 
		  	"complete": { 
			 "start": ">Статус:<",
             "end": ">Жанр:<",
			   "values": [ "Завершена", "Переведена", "Сингл"] },
			
            "ongoing": { 
			"start": ">Статус:<",
             "end": ">Жанр:<",
                "value": "Выпускается" }
			 },
			 
			 
      "summary":{
         "skip":">Описание:<",
         "token1":"class=\"manga-description entry\">",
         "token2":"</div>",
         "replace":{
            "match":"<.*?>",
            "text":""
         }
      },
      "add_tags":{
         "start":">Жанр:<",
         "end":">Год:<",
         "token1":"\">",
         "token2":"</"
      },
      "is_mature":{
         "values":[
            "эротика",
            "сэйнэн",
            "юри",
			"этти",
            "яой"
         ]
      },
	  
	
	  
	  
	  
		
		"add_chapter":{
        "start": "<head>",
		 "end": "class=\"chapters-infinite-pagination\"",
        
		 
         "next": "chapter-item",
		 
		                 
         "link":{
			
            "token1":"href=\"",
            "token2":"\"",
			"replace": {"prefix" : "https://mangapoisk.net"}
         },
		 
		"uniq":{
            "skip":"href=\"",
            "token1":"/chapter/",
            "token2":"\""
        },
		 
        "title":{
            "skip":"href=\"",
            "token1":"class=\"chapter-title\">",
            "token2":"</a>",
			"replace": { "match": "</span>",	"text": ""}
        }
      }
	  
	  
   }
   ],
   "chapter_complete":{
    "add_numeration": true,
        "headers": {
			"Accept": "*/*",
			"Referer": "https://mangapoisk.net/"
		},
     
      "images":true,
"add_pages":
[
	{
		"start": "chapter-images\">",
		"before": "id=\"",
		"token1": "<img src=\"https://static2.mangapoisk.net/pages/",
		"token2": "\"",
		"end": "<div class=\"row text-center\">",
		"replace":
