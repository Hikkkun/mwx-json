{
	"id": 40118,
	"version": 2,
	"title": "MLate",
	"host": "https://mlate.ru",
	"public_link": "https://mlate.ru/search/?cat=2",
	"folder": "mlate",
	"name": "mlate",
	"chapters_order": true,
	"language": "ru",
	"encoding": "utf-8",
    
	"icon_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/img/mlate.png",
	"self_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/parsers/mlate.json",
	"self_link2": "https://fant.my1.ru/mwparser/mwx/json/mlate.json",
	"catalog_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/catalogs/mlate.zip",
	"file_type": "sqlite",

	"author": {
		"name": "Veopot",
		"email": "fant-net@ya.ru"
	},

	"authorization_complete": {
		"logo": "https://mlate.ru/i/logo-v7.gif",
		"registration": "https://mlate.ru/register",
		"expires": 31536000000,
		"is_redirect": true,

		"values": {

			"login[login]": {
				"label": "Логин",
				"hint": "Введите свой логин с сайта mlate!",
				"kind": "text"
			},

			"login[pass]": {
				"label": "Пароль",
				"hint": "Введите свой пароль с сайта mlate!",
				"kind": "password"
			},
			"headers": {
				"Accept": "*/*"
			},

			"mature": {
				"forward": "cookie",
				"value": "c3a2ed4b199a1a15f5a5483504c7a75a7030dc4bi%3A1%3B"
			}
		},

		"request": {
			"method": "post",
			"url": "https://mlate.ru"
		},
		"response_check": {
			"headers": {
				"Set-Cookie": "@%22login"

			}
		}
	},

	"manga_search_complete": {
		"search_link": "https://mlate.ru/search/index/t/$query$/cat/0/s_lang/0/t_lang/0/sort/4/n_chapters//Book_page/$page$",
		"query_type": "GET",
		"add_manga": {
			"start": "<ul class='search-results'>",
			"next": "<a href='/book/",

			"link": {
				"skip": "<a",
				"token1": "href='/book/",
				"token2": "'>",
				"replace": {
					"prefix": "https://mlate.ru/book/"
				}
			},
			"title": {
				"skip": "href='",
				"token1": "'>",
				"token2": "</"
			},
			"uniq": {
				"skip": "href='",
				"token1": "book/",
				"token2": "'>"
			},

			"summary": {
				"after": "<div id='book",
				"token1": "'>",
				"token2": "</div>",
				"replace": {
					"match": "<.*?>",
					"text": ""
				}
			},

			"cover": {
				"before": "href='",
				"token1": "src='",
				"token2": "'",
				"replace": {
					"prefix": "https://mlate.ru/"
				}
			}
		}
	},

	"manga_list_complete": {
		"iterator": {
			"base_url": "https://mlate.ru/search/index/cat/2/Book_page/",
			"append_nums": {
				"from": 1,
				"to": 15,
				"sufix": "/"
			}
		},


		"add_manga": {
			"start": "</h1>",
			"end": "</h5>",
			"next": "<p class='book-tooltip'",

			"link": {
				"token1": "href='/book/",
				"token2": "'>",
				"replace": {
					"prefix": "https://mlate.ru/book/"
				}
			},
			"title": {
				"skip": "href='/book/",
				"token1": "'>",
				"token2": "</"
			},
			"uniq": {
				"skip": "href='",
				"token1": "book/",
				"token2": "'>"
			}
		}

	},

	"manga_complete": {
		"headers": {
			"cookie": "mature=c3a2ed4b199a1a15f5a5483504c7a75a7030dc4bi%3A1%3B"
		},

		"title": {
			"skip": "class=\"nav nav-tabs book-header\"",
			"token1": "h1>",
			"token2": "</",
				"replace" : { "match" : "Ранобэ","text":"" }
		},
		
		
		
		"uniq": {
			"skip": "var Book = new CBook",
			"token1": "id:",
			"token2": ","
		},
		"summary": {
			"after": "property=\"ranobe:description\"",
			"token1": "content=\"",
			"token2": "\""

		},
		"author": {
			"skip": ">Автор:</",
			"token1": "<em>",
			"token2": "</em>",
			"replace": {
				"match": "<.*?>",
				"text": ""
			}
		},

		"status": {
			"empty": {
				"start": "<div class='alert alert-info'",
				"end": "id=\"Chapters\">",
				"value": "В этом переводе ещё не создано ни одной главы."
			},
			"complete": {
				"start": "dt>Состояние перевода:</dt",
				"end": "dt>Перевод:</dt",
				"values": [">Завершён", ">Переведена"]
			},
			"ongoing": {
				"start": "dt>Состояние перевода:</dt",
				"end": "dt>Перевод:</dt",
				"values": [">В работе", ">Ожидание", ">Перерыв"]
			},
			"default": "unknown"
		},



		"add_tags": {
			"start": "<dt>Тэги:</dt>",
			"end": "</dd>",
			"after": "<a href=\"/search?genres",
			"token1": "\">",
			"token2": "</"
		},
		"add_genres": {
			"start": "<dt>Жанры:</dt>",
			"end": "</dd>",
			"after": "<a href=\"/search?tags",
			"token1": "\">",
			"token2": "</"
		},
		"is_mature": {
			"start": "<div id=\"Info\">",
			"end": "class=\"dropdown-menu\"",
			"values": ["+18", "18+", "эротика"]
		},
		"cover": {
			"skip": "<div class=\"images\"",
			"token1": "<img src=\"/i/",
			"token2": "\"",
				"replace": {
					"prefix": "https://mlate.ru/i/"
				}
		},

		"content_type": {
			"default": "light_novels"
		},


		"add_chapter": {
			"start": "<tbody>",
			"end": ">Обсуждение:</",
			"next": "<td class=",
			"link": {
				"before": "title='Получить",
				"token1": "href='",
				"token2": "'"
			},
			"uniq": {
				"before": "title='Получить",
				"token1": "'t'>",
				"token2": "</a",
				"replace": [{
						"match": "<.*>",
						"text": ""
					},
					{
						"match": "Главы",
						"text": "Глава"
					},
					{
						"match": "[^a-zA-Z0-9]",
						"text": ""
					}
				]
			},

			"title": {
				"before": "title='Получить",
				"token1": "'t'>",
				"token2": "</a",
				"replace": [{
						"match": "<.*>",
						"text": ""
					},
					{
						"match": "Главы",
						"text": "Глава"
					}
				]
			}
		}

	},


	"chapter_complete": {
		"headers": {
			"cookie": "mature=c3a2ed4b199a1a15f5a5483504c7a75a7030dc4bi%3A1%3B",
			"cookie2": "mwatcher_d=sldotndjssf"
		},

		"images": true,
		"add_pages": {
			"start": ">Настройки</",
			"token1": "action=\"/book/",
			"token2": "/ready",
			"end": "</html>"
		},
		"prefix": "https://mlate.ru/book/",
		"sufix": "/download?format=f&enc=UTF-8&key=qwerty&title=/ranobe.fb2"
	},


	"test": [{
			"proc": "manga_list_complete",

			"assert": {
				"mangas": [{

					"link": "https://mlate.ru/book/27",
					"uniq": "27"
				}, {
					"link": "https://mlate.ru/book/333"
				}],
				"mangas_count": ">60"
			}
		},

		{
			"proc": "manga_complete",
			"link": "https://mlate.ru/book/1",
			"title": "Global Evolution / Глобальная эволюция",
			"assert": {
				"cover": "https://mlate.ru/i/book/20/6/26186.jpg",
				"summary": "@^(Таинственная черная дыра)",
				"genres": "@fantasy",
				"status": "ongoing",
				"chapters_count": ">=400",
				"genres_count": ">=2",


				"chapters": [
		{
            "folder": "1/",
            "link": "https://mlate.ru/book/1/21/ready",
            "title": "Глава 1: Забудьте об этой эпохе (ее можно пропустить)."
        },
        {
            "link": "https://mlate.ru/book/1/22/ready",
            "title": "Глава 2: Я вернулся!"
        }
				]
			}
		},
        {
			"proc": "chapter_complete",
			"link": "https://mlate.ru/book/1/21/ready",
			"assert": {
				"image": "https://mlate.ru/book/1689/250072/download?format=f&enc=UTF-8&key=qwerty&title=/ranobe.fb2"
			}

		},

		{
			"proc": "chapter_complete",
			"link": "https://mlate.ru/book/149/23125/ready",
			"assert": {
				"image": "https://mlate.ru/book/149/23125/download?format=f&enc=UTF-8&key=qwerty&title=/ranobe.fb2"
			}

		}
	]
}